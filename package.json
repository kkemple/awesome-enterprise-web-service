{
  "name": "@kkemple/awesome-enterprise-web-service",
  "version": "0.0.1",
  "description": "An enterprise grade web service example",
  "main": "lib/index.js",
  "scripts": {
    "audit": "nsp check",
    "compile": "rimraf lib && babel src --out-dir lib --source-maps inline",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "db:migration:create": "sequelize migration:create",
    "db:seed": "sequelize db:seed",
    "db:seed:all": "sequelize db:seed:all",
    "db:seed:create": "sequelize seed:create",
    "db:seed:undo": "sequelize db:seed:undo",
    "db:seed:undo:all": "sequelize db:seed:undo:all",
    "lint": "eslint .",
    "package": "npm shrinkwrap --dev && node bin/fix-shrinkwrap && shrinkpack",
    "pretest": "npm run lint",
    "pretest:unit": "npm run lint",
    "start": "npm run db:migrate && npm run db:seed:all && node lib/index.js",
    "test": "npm run db:migrate && npm run db:seed:all && istanbul cover tape -- -r babel-register \"src/**/*.test.js\"",
    "test:coverage": "npm run test && codeclimate-test-reporter < coverage/lcov.info && codecov",
    "test:unit": "tape -r babel-register \"src/**/unit.test.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/awesome-enterprise-web-service.git"
  },
  "keywords": [],
  "author": "Kurtis Kemple <kurtiskemple@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/awesome-enterprise-web-service/issues"
  },
  "homepage": "https://github.com/awesome-enterprise-web-service#readme",
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "catbox-redis": "^2.0.2",
    "good": "^7.0.1",
    "good-console": "^6.1.2",
    "hapi": "^15.0.1",
    "hapi-auth-basic": "^4.2.0",
    "hapi-auth-jwt2": "^7.0.1",
    "hapi-statsd": "^5.0.1",
    "hapi-swagger": "^7.0.1",
    "inert": "^4.0.0",
    "joi": "^9.0.0",
    "jsonwebtoken": "^7.0.1",
    "lodash.assign": "^4.0.9",
    "lodash.flattendeep": "^4.2.0",
    "lodash.omit": "^4.3.0",
    "lodash.reduce": "^4.4.0",
    "minimatch": "^3.0.2",
    "mysql": "^2.11.1",
    "negotiator": "^0.6.1",
    "newrelic": "^1.28.1",
    "sequelize": "^3.23.4",
    "sequelize-cli": "^2.4.0",
    "socket.io": "^1.4.8",
    "socketio-jwt": "^4.5.0",
    "uuid": "^2.0.2",
    "vision": "^4.1.0",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "codeclimate-test-reporter": "^0.4.0",
    "codecov": "^1.0.1",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^3.0.0",
    "eslint-plugin-react": "^6.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "nock": "^8.1.0",
    "nsp": "^2.5.0",
    "rimraf": "^2.5.2",
    "shrinkpack": "^0.17.0",
    "sinon": "^1.17.4",
    "tape": "^4.6.0"
  },
  "optionalDependencies": {
    "fsevents": "^1.0.12"
  }
}
